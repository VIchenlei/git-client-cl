// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "editor.snippetSuggestions": "top",
  "javascript.validate.enable": false,
  // "javascript.format.enable": true,
  "standard.validate": [
    "javascript",
    {
      "language": "html",
      "autoFix": true
    }
  ],
  "standard.options": {
    "plugins": [
      "html"
    ]
  },
  // "files.associations": {
  //   "*.vue": "html"
  // },
  "standard.enable": true,
  "standard.run": "onType",
  "standard.autoFixOnSave": true
}
